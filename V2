-- Sound
local clickSound = Instance.new("Sound", SoundService)
clickSound.SoundId = "rbxassetid://12222030"
clickSound.Volume = 0.5

-- GUI
local gui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
gui.Name = "KeyUI"
local frame = Instance.new("Frame", gui)
frame.Size = UDim2.new(0.35, 0, 0.3, 0)
frame.Position = UDim2.new(0.325, 0, 0.35, 0)
frame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
frame.BorderSizePixel = 0
frame.ClipsDescendants = true
frame.AnchorPoint = Vector2.new(0.5, 0.5)
frame.Position = UDim2.new(0.5, 0, 0.5, 0)
frame.BackgroundTransparency = 1
frame:TweenSizeAndPosition(
    UDim2.new(0.35, 0, 0.3, 0),
    UDim2.new(0.5, 0, 0.5, 0),
    Enum.EasingDirection.Out,
    Enum.EasingStyle.Back,
    0.4,
    false,
    function()
        TweenService:Create(frame, TweenInfo.new(0.3), {BackgroundTransparency = 0}):Play()
    end
)

local uicorner = Instance.new("UICorner", frame)
uicorner.CornerRadius = UDim.new(0, 20)

local box = Instance.new("TextBox", frame)
box.Size = UDim2.new(0.8, 0, 0.2, 0)
